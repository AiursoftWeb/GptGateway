<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <AssemblyName>Aiursoft.GptGateway</AssemblyName>
    <RootNamespace>Aiursoft.GptGateway</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <IsPackable>false</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>GptGateway is a OpenAI ChatGPT API gateway. Allowing GPT to call tools like search engines.</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Aiursoft.Canon" Version="9.0.1" />
    <PackageReference Include="Aiursoft.DbTools.Sqlite" Version="9.0.5" />
    <PackageReference Include="Aiursoft.WebTools" Version="9.0.0" />
    <PackageReference Include="Microsoft.Azure.CognitiveServices.Search.WebSearch" Version="2.0.0" />
  </ItemGroup>
</Project>